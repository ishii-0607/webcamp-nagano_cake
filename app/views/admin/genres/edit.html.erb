<div class="container">
  <div class="row">
    <div class="mx-auto">
      <h2>ジャンル編集</h2>
      <%= form_with model: @genre, url: admin_genre_path, method: :patch, local:true do |f| %>
        <label for="InputGenre">ジャンル名</label>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存" %>
      <% end %>
    </div>
  </div>
</div>
